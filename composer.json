{
    "name": "schivei/php-go",
    "description": "php-go allows to call Go code from PHP, with minimal code boilerplate.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Schivei\\PhpGo\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Elton Schivei Costa",
            "email": "costa@elton.schivei.nom.br"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0"
    },
    "scripts": {
        "post-install-cmd": "go install ../php-go && php-go",
        "post-update-cmd": "go install ../php-go && php-go"
    },
    "version": "1.0.0",
    "extra": {
        "go": {
            "package": "github.com/schivei/php-go",
            "version": "v1.0.0"
        }
    }
}
